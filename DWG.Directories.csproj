<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>DWG.Directories</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>Kat Marchán</Authors>
    <Copyright>Kat Marchán</Copyright>
    <PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Description>Cross-platform directories support.</Description>
    <PackageProjectUrl>https://github.com/zkat/DWG.Directories</PackageProjectUrl>
    <RepositoryUrl>https://github.com/zkat/DWG.Directories</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>xdg directories</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="/" />
  </ItemGroup>

</Project>
